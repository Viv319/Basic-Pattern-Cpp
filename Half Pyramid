/* print this pattern

*
**
***
****
*****
******

when n=6
*/

#include <bits/stdc++.h> 

using namespace std; 
/* For a given array arr[], returns the maximum j â€“ i such that 
	arr[j] > arr[i] */


int main()
{
    int n,i,j;
    
    cin>>n;
    
    for(i=0;i<=n;i++)
    {
        for(j=0;j<i;j++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
}
