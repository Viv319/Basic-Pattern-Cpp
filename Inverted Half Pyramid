/* print this pattern

******
*****
****
***
**
*

when n=6
*/


#include <bits/stdc++.h> 

using namespace std; 
/* For a given array arr[], returns the maximum j â€“ i such that 
	arr[j] > arr[i] */


int main()
{
    int n,i,j;
    
    cin>>n;
    
    for(i=n;i>0;i--)
    {
        for(j=i;j>0;j--)
        {
            cout<<"*";
        }
        cout<<endl;
    }
}
